{
  "name": "class-reunion",
  "version": "1.0.0",
  "description": "This is a website for a class reunion as part of a school assignment.",
  "main": "index.html",
  "scripts": {
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "scss": "node-sass -o css/ css/",
    "lite": "lite-server",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cgfanning/stunning-adventure.git"
  },
  "author": "Chantele Fanning",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cgfanning/stunning-adventure/issues"
  },
  "homepage": "https://github.com/cgfanning/stunning-adventure#readme",
  "devDependencies": {
    "lite-server": "^2.3.0",
    "node-sass": "^4.12.0",
    "onchange": "^3.3.0",
    "parallelshell": "^3.0.1"
  },
  "dependencies": {
    "bootstrap": "^4.3.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "popper.js": "^1.12.9"
  }
}
